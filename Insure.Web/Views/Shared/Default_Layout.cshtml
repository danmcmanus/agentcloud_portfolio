
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Insure.Secure</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/Site.og.css" rel="stylesheet" />
    <link href="http://fonts.googleapis.com/css?family=Source+Sans+Pro:400,700,400italic,700italic" rel="stylesheet" type="text/css">
    <link href="~/Content/style.css" rel="stylesheet" />
    <link href="~/Content/toggle-switch.css" rel="stylesheet" />
</head>
<body>

        <div class="navbar navbar-brand">
            
                <div class="navbar navbar-inner">
                    
                        <ul class="nav">
                            <li class="divider-vertical"></li>
                            <li>@Html.ActionLink("Home", "Index", "Home")</li>
                            <li class="divider-vertical"></li>
                            <li>@Html.ActionLink("Client Stats", "About", "Home")</li>
                            <li class="divider-vertical"></li>
                            <li>@Html.ActionLink("Clients", "Index", "User")</li>
                            <li class="divider-vertical"></li>
                            <li>@Html.ActionLink("Policies", "Index", "Policy")</li>
                            <li class="divider-vertical"></li>
                            <li>@Html.ActionLink("Companies","Index","Company")</li>
                            <li class="divider-vertical"></li>
                            <li class="dropdown">
                                <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                    Manage
                                    <b class="caret"></b>
                                </a>
                                <ul class="dropdown-menu">
                                    <li>@Html.Partial("_LoginPartial")</li>
                                    <li>@Html.ActionLink("Calculator","Index","Calculator" )</li>
                                    <li>@Html.ActionLink("Companies","Index","Company")</li>
                                    <li>@Html.ActionLink("Salesforce Contacts", "Index", "Contacts")</li> 
                                </ul>
                            </li>
                            <li class="divider-vertical"></li>
                        </ul>
                        <form class="navbar-search pull-right">
                            <input type="text" class="search-query" placeholder="Search" name="searchQuery" id="nav-search">
                        </form>
                    </div> <!-- /.navbar-inner -->
                </div> <!-- /.navbar -->
            

            
    
    <div class="container body-content">

        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - Insure.Secure</p>
        </footer>
    </div>
    @*<script src="~/Scripts/bootstrap.js"></script>*@
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/jquery.validate.js"></script>
    <script src="~/Scripts/jquery-2.2.2.min.js"></script>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)

    <script type="text/javascript" async="async" defer="defer" data-cfasync="false" src="https://mylivechat.com/chatinline.aspx?hccid=73615552"></script>

</body>          

